    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "komet": "^0.1.4",
    "komet-karma": "^0.2.5",
    "pob-babel": "^17.2.0",
    "pob-release": "^3.1.0",
    "react": "^15.5.4",
    "react-alp-link": "^0.5.0",
    "ynnub": "^0.2.5"
